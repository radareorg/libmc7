#ifndef LIB_SIMATIC_TYPES_H
#define LIB_SIMATIC_TYPES_H

#ifdef __cplusplus
extern "C" {
#endif1

#define bool_t int
#define st8_t  signed char
#define st16_t short
#define st32_t int
#define st64_t long long
#define ut8_t  unsigned char
#define ut16_t unsigned short
#define ut32_t unsigned int
#define ut64_t unsigned long long

#ifdef __cplusplus
}
#endif

#endif /* LIB_SIMATIC_TYPES_H */